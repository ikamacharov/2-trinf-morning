    </Routine>
    <Routine name="encouragement">
      <CodeComponent name="code_2">
        <Param name="Before Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="trial_a=0&amp;#10;msgone=&quot;&quot;&amp;#10;msgtwo=&quot;&quot;&amp;#10;msgthree=&quot;&quot;&amp;#10;msgfour=&quot;&quot;" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="trial_a = 0;&amp;#10;msgone = &quot;&quot;;&amp;#10;msgtwo = &quot;&quot;;&amp;#10;msgthree = &quot;&quot;;&amp;#10;msgfour = &quot;&quot;;&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="trial_a = (trial_a + 1);&amp;#10;if ((trial_a === 18)) {&amp;#10;    msgone = &quot;Not to bad...but not enough, please try again !&quot;;&amp;#10;}&amp;#10;if ((trial_a !== 18)) {&amp;#10;    msgone = &quot;&quot;;&amp;#10;}&amp;#10;if ((trial_a === 54)) {&amp;#10;    msgtwo = &quot;You're getting better, but your accuracy is still not high enough hang in there !&quot;;&amp;#10;}&amp;#10;if ((trial_a !== 54)) {&amp;#10;    msgtwo = &quot;&quot;;&amp;#10;}&amp;#10;if ((trial_a === 108)) {&amp;#10;    msgthree = &quot;I promise, you're not stucked into an infinite loop (^_^)&quot;;&amp;#10;}&amp;#10;if ((trial_a !== 108)) {&amp;#10;    msgthree = &quot;&quot;;&amp;#10;}&amp;#10;if ((trial_a === 162)) {&amp;#10;    msgfour = &quot;Almost done !&quot;;&amp;#10;}&amp;#10;if ((trial_a !== 162)) {&amp;#10;    msgfour = &quot;&quot;;&amp;#10;}&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="trial_a=trial_a+1&amp;#10;&amp;#10;if trial_a==18:&amp;#10;    msgone=&quot;Not to bad...but not enough, please try again !&quot;&amp;#10;&amp;#10;if trial_a!=18:&amp;#10;    msgone=&quot;&quot;&amp;#10;&amp;#10;if trial_a==54:&amp;#10;    msgtwo=&quot;You're getting better, but your accuracy is still not high enough hang in there !&quot;&amp;#10;&amp;#10;if trial_a!=54:&amp;#10;    msgtwo=&quot;&quot;&amp;#10;&amp;#10;if trial_a==108:&amp;#10;    msgthree=&quot;I promise, you're not stucked into an infinite loop (^_^)&quot;&amp;#10;&amp;#10;if trial_a!=108:&amp;#10;    msgthree=&quot;&quot;&amp;#10;&amp;#10;if trial_a==162:&amp;#10;    msgfour=&quot;Almost done !&quot;&amp;#10;&amp;#10;if trial_a!=162:&amp;#10;    msgfour=&quot;&quot;" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Auto-&gt;JS" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="name" updates="None" val="code_2" valType="code"/>
      </CodeComponent>
    </Routine>
